{
  "name": "cuckoo",
  "version": "1.0.0",
  "description": "Configure times for specific music to play",
  "main": "index.tsx",
  "scripts": {
    "build": "tsc",
    "start-server": "nodemon dist/server/server.js --watch dist/server/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\", \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runninggee57/cuckoo.git"
  },
  "keywords": [
    "cuckoo",
    "play",
    "music",
    "specific",
    "time"
  ],
  "author": "Phil Morley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runninggee57/cuckoo/issues"
  },
  "homepage": "https://github.com/runninggee57/cuckoo#readme",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "react-scripts": "^4.0.3",
    "sqlite3": "^5.0.2",
    "typescript": "^4.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
